<div metal:use-macro="_LAYOUT">
<div metal:fill-slot="content">
  <table border="1">
    <tr>
      <th>User's Name</th>
      <th>Email Address</th>
    </tr>
    <tr tal:repeat="user users">
      <td>${user.name}</td>
      <td><a href="${request.route_path('user_item', username=user.username)}">${user.username}</a></td>
    </tr>
  </table>
</div>
</div>
